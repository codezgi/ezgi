#include <iostream>
using namespace std;

int boxVolume(int length=1, int width=1 , int height=1 );
int main()
{
    cout<<"the default box volume is : "<<boxVolume();
    cout<<"the valume of a box with length 10, width 1 and height 1 is : "<<boxVolume(10)<<endl;
    cout<<"the volume of box with l=10,w=5,h=1 : "<<boxVolume(10,5)<<endl;
    cout<<"the volume of box with l=10,w=5,h=2 : "<<boxVolume(10,5,2)<<endl;
    return 0;
}
int boxVolume(int length,int width,int height)
{
    return length*width*height;
}
