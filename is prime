#include <iostream>
using namespace std;


bool isPrimeNumber(int number){
    bool isPrimeFlag=true;
    for(int i=2; i<number; i++){
        if(number%i==0){
            isPrimeFlag=false;
            break;
        }
      
  }
  return isPrimeFlag;
}

int main(){
    int number;
    cout<<"number :";
    cin>>number;

    bool isPrimeFlag=isPrimeNumber(number);
    
    if(isPrimeFlag)
        cout<<"prime number "<<endl;
    
    else
        cout<<"not prime number "<<endl;


        
        return 0;
    
}
