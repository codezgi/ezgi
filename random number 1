#include <iostream>
#include <iomanip>
using namespace std;
using std::setw;
#include <cstdlib>
using std::rand;
int main()
{

  for(int counter=1;counter<=20;counter++)
  {
    cout<<setw(10)<<(1+rand()%6);// calling function rand and we says keep a number between 1 and 6
    if(counter%5==0){ //if counter divisble by 5, start a new line of the output
        cout<<endl;
    }
  }
    return 0;
}
